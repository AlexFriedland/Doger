<h2>Create a New Dog:</h2>
<br>
<form action="/newdog" method="POST">
  <label>Name:</label>
  <input type="text" name="name">
</form>

<label><h4>Walks</h4></label>
  <% Walk.all.each do |walk| %>
    <label><%= walk.miles.to_f %></label>
    <input type="checkbox" id="walk_<%=walk.id%>" name="dog[walk_ids][]" value="<%= walk.id %>"><br>

  <% end %>

<br></br>
<input type="submit" value="submit" name="submit">
